# Production Environment Configuration
# These values are embedded at build time

REACT_APP_API_URL=https://quirk-backend-production.up.railway.app/api/v1
REACT_APP_ENVIRONMENT=production
REACT_APP_DEALERSHIP=Quirk Chevrolet
REACT_APP_KIOSK_ID=PROD-KIOSK

# Error Tracking (Sentry)
REACT_APP_SENTRY_DSN=

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_AI_CHAT=true

# Version (set by CI/CD)
REACT_APP_VERSION=$npm_package_version
REACT_APP_BUILD_TIME=
